# Add these to your .env for Paypack integration
PAYPACK_TOKEN=your_paypack_bearer_token
# Optionally, add PAYPACK_API_KEY and PAYPACK_API_SECRET if you want to implement dynamic token retrieval

# Twilio Credentials
TWILIO_ACCOUNT_SID=your_account_sid_here
TWILIO_AUTH_TOKEN=your_auth_token_here
TWILIO_PHONE_NUMBER=your_twilio_phone_number_here

# Dialogflow Credentials (if using)
DIALOGFLOW_PROJECT_ID=your_project_id_here
GOOGLE_APPLICATION_CREDENTIALS=path_to_your_service_account_key.json

# Server Configuration
FLASK_ENV=development
FLASK_DEBUG=1
PORT=5001

# Soil Sensor API Configuration
SOIL_SENSOR_API_KEY=your_secure_api_key_here
